{
    "name": "nozavroni/collect",
    "description": "A very simple collection library for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Visinoni",
            "email": "luke.visinoni@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/nozavroni/collect/issues"
    },
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.1",
        "phpunit/phpunit": "^5.7",
        "phpdocumentor/phpdocumentor": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Noz\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Noz\\Tests\\": "tests/Tests"
        },
        "files": [
            "tests/functions.php"
        ]
    }
}
